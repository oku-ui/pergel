{
  "name": "@pergel/nuxt",
  "type": "module",
  "version": "0.8.17",
  "description": "Nuxt Full Stack modules. Start your next nuxt project fast and strong.",
  "author": "Mehmet @productdevbook",
  "license": "MIT",
  "funding": "https://github.com/sponsors/productdevbook",
  "homepage": "https://github.com/oku-ui/pergel",
  "repository": {
    "type": "git",
    "url": "https://github.com/oku-ui/pergel.git"
  },
  "bugs": "https://github.com/oku-ui/pergel/issues",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/module.d.mts",
        "default": "./dist/module.mjs"
      },
      "require": {
        "types": "./dist/module.d.ts",
        "default": "./dist/module.cjs"
      }
    },
    "./runtime/modules": {
      "types": "./dist/runtime/modules/index.d.ts"
    },
    "./types": {
      "types": "./dist/types.d.ts"
    },
    "./*": "./*"
  },
  "main": "./dist/module.cjs",
  "module": "./dist/module.mjs",
  "types": "./dist/module.d.ts",
  "files": [
    "dist",
    "lib"
  ],
  "scripts": {
    "stub": "pnpm nuxt-build-module build --stub && nuxt-module-build prepare",
    "prepack": "pnpm nuxt-module-build build && pnpm run client:build",
    "dev": "pnpm nuxi dev playground",
    "dev:build": "pnpm nuxi build playground",
    "build": "pnpm nuxt-module-build build",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground && nuxi prepare client",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "client:build": "nuxi generate client",
    "client:dev": "nuxi dev client --port 3300",
    "release:latest": "pnpm prepack && pnpm bumpp --commit='version(nuxt): release %s' --no-tag && pnpm publish",
    "release:beta": "pnpm release --tag beta",
    "release:alpha": "pnpm release --tag alpha",
    "build:script": "pnpm --filter './build-script/**' build",
    "types:docker-compose": "esno scripts/compose-spec.ts"
  },
  "dependencies": {
    "@nuxt/devtools": "^1.0.8",
    "@nuxt/devtools-kit": "^1.0.8",
    "@nuxt/kit": "^3.10.0",
    "@vueuse/core": "^10.7.2",
    "defu": "^6.1.4",
    "globby": "^14.0.0",
    "local-pkg": "^0.5.0",
    "minimatch": "^9.0.3",
    "pathe": "^1.1.2",
    "sirv": "^2.0.4",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "@aws-sdk/client-ses": "^3.504.0",
    "@faker-js/faker": "^8.4.0",
    "@graphql-yoga/urql-exchange": "^3.1.1",
    "@iconify-json/carbon": "^1.1.28",
    "@iconify-json/ph": "^1.1.11",
    "@json2csv/node": "^7.0.5",
    "@lucia-auth/adapter-drizzle": "1.0.1",
    "@lucia-auth/adapter-postgresql": "3.1.0",
    "@nuxt/devtools-ui-kit": "^1.0.8",
    "@nuxt/module-builder": "^0.5.5",
    "@nuxt/schema": "^3.10.0",
    "@nuxt/test-utils": "^3.11.0",
    "@pergel/cli": "^0.6.2",
    "@pergel/graphql": "workspace:^",
    "@pergel/module-box": "workspace:^",
    "@pergel/module-s3": "workspace:^",
    "@types/node": "^20.11.16",
    "@types/node-cron": "^3.0.11",
    "@urql/core": "^4.2.3",
    "@urql/exchange-execute": "^2.2.2",
    "@urql/vue": "^1.1.2",
    "bullmq": "^5.1.8",
    "dotenv": "^16.4.1",
    "drizzle-kit": "^0.20.14",
    "drizzle-orm": "^0.29.3",
    "esbuild-plugin-file-path-extensions": "^2.0.0",
    "graphql": "^16.8.1",
    "h3": "^1.10.1",
    "ioredis": "^5.3.2",
    "json-schema-to-typescript": "^13.1.2",
    "lucia": "3.0.1",
    "node-cron": "^3.0.3",
    "nuxt": "^3.10.0",
    "oslo": "^1.0.4",
    "p-timeout": "^6.1.2",
    "pg": "^8.11.3",
    "postgres": "^3.4.3",
    "scule": "^1.2.0",
    "typescript": "^5.3.3",
    "vitest": "^1.2.2"
  },
  "publishConfig": {
    "access": "public"
  }
}

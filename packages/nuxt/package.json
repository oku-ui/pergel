{
  "name": "@pergel/nuxt",
  "type": "module",
  "version": "0.3.1",
  "description": "Nuxt Full Stack modules. Start your next nuxt project fast and strong.",
  "author": "Mehmet @productdevbook",
  "license": "MIT",
  "funding": "https://github.com/sponsors/productdevbook",
  "homepage": "https://github.com/oku-ui/pergel",
  "repository": {
    "type": "git",
    "url": "https://github.com/oku-ui/pergel.git"
  },
  "bugs": "https://github.com/oku-ui/pergel/issues",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    },
    "./drizzle/*": {
      "types": "./dist/drizzle/*.d.ts",
      "import": "./dist/drizzle/*.mjs",
      "require": "./dist/drizzle/*.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "typesVersions": {
    "*": {
      "*": [
        "dist/*"
      ],
      "./drizzle/*": [
        "dist/drizzle/*"
      ]
    }
  },
  "scripts": {
    "stub": "nuxt-build-module build --stub && nuxt-module-build prepare",
    "prepack": "nuxt-module-build build && npm run client:build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "build": "nuxt-module-build build",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground && nuxi prepare client",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "client:build": "nuxi generate client",
    "client:dev": "nuxi dev client --port 3300",
    "release": "pnpm dev:prepare && pnpm prepack && pnpm bumpp --commit='version(nuxt): release %s' --no-tag && pnpm publish"
  },
  "dependencies": {
    "@nuxt/devtools": "^1.0.5",
    "@nuxt/devtools-kit": "^1.0.5",
    "@nuxt/kit": "^3.8.2",
    "@vueuse/core": "^10.7.0",
    "defu": "^6.1.3",
    "minimatch": "^9.0.3",
    "pathe": "^1.1.1",
    "sirv": "^2.0.3"
  },
  "devDependencies": {
    "@aws-sdk/client-ses": "^3.465.0",
    "@iconify-json/carbon": "^1.1.24",
    "@iconify-json/ph": "^1.1.9",
    "@json2csv/node": "^7.0.4",
    "@nuxt/devtools-ui-kit": "^1.0.5",
    "@nuxt/module-builder": "^0.5.4",
    "@nuxt/schema": "^3.8.2",
    "@nuxt/test-utils": "^3.8.1",
    "@pergel/graphql": "workspace:^",
    "@pergel/module-s3": "workspace:^",
    "@types/node": "^20.9.3",
    "@types/node-cron": "^3.0.11",
    "bullmq": "^4.15.2",
    "esbuild-plugin-file-path-extensions": "^2.0.0",
    "node-cron": "^3.0.3",
    "nuxt": "^3.8.2",
    "p-timeout": "^6.1.2",
    "vitest": "^0.33.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
